# PROGRAM_5.

'''
rows=int(input("Enter no. of rows:"))
for  i in range(rows):
    num=i+1
    for j in range (rows):
        print(num,end=" ")
        num+=1
    print()
print()
'''

# PROGRAM_6.

'''
rows=int(input("Enter no. of rows:"))
for i in range(rows):
    for j in range(rows):
        if(i%2==0):
            print("#",end="  ")
        else:
            print("@",end="  ")
    print()
print()
'''

# PROGRAM_7.

