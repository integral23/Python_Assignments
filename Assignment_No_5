# PROGRAM_1.
'''

rows=int(input("Enter no. of rows:"))
for i in range(rows):
    for j in range(rows):
        print("*",end=" ")
    print()
print()
'''


# PROGRAM_2.

'''
rows=int(input("Enter no. of rows:"))
num=1
for i in range(rows):
    for j in range(rows):
        if(num<10):
            print(num,end="  ")
            num+=1
        else:
            print(num,end=" ")
            num+=1
    print()
print()
'''


# PROGRAM_3.

'''
rows=int(input("Enter no. of rows:"))
for i in range(rows):
    num=i+1
    for j in range(rows):
        print(num,end=" ")
    print()
print()
'''


