# PROGRAM_9.

'''
rows=int(input("Enter no. of rows:"))
num=1
for i in range(rows):
    for j in range(rows):
        print(num,end=" ")
        num+=2
    print()
    num-=4
print()
'''


# PROGRAM_10.

'''
rows=int(input("Enter no. of rows:"))
num=1
for i in range(rows):
    for j in range(rows):
        if(num<10):
            print(num,end="  ")
        else:
            print(num,end=" ")
        num+=2
    print()
    num-=4
print()
'''
